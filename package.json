{
    "name": "solar-eclipse",
    "version": "1.0.1",
    "description": "A Preact-enabled ABC News story",
    "license": "MIT",
    "private": true,
    "contributors": [
        "Joshua Byrd <phocks@gmail.com>"
    ],
    "aunty": {
        "deploy": {
            "contentftp": {
                "files": "**",
                "from": "build",
                "to": "/www/res/sites/news-projects/<name>/<id>"
            }
        }
    },
    "scripts": {
        "start": "aunty serve"
    },
    "devDependencies": {
        "@abcnews/aunty": "4.2.x",
        "prettier": "^1.5.3"
    },
    "dependencies": {
        "d3-geo": "^1.6.4",
        "d3-geo-projection": "^2.2.0",
        "d3-request": "^1.0.5",
        "d3-scale": "^1.0.6",
        "d3-selection": "^1.1.0",
        "d3-shape": "^1.2.0",
        "d3-timer": "^1.0.6",
        "preact": "^8.2.1",
        "preact-compat": "^3.16.0",
        "promise-polyfill": "6.0.2",
        "smoothscroll": "^0.3.0",
        "topojson": "^3.0.0"
    }
}
